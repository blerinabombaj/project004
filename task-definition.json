{
  "family": "django-service",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::444062204470:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::444062204470:role/ecsTaskRole",
  "containerDefinitions": [{
    "name": "django",
    "image": "444062204470.dkr.ecr.eu-north-1.amazonaws.com/project004:latest",
    "portMappings": [{"containerPort": 8000, "protocol": "tcp"}],
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/django-cluster/django-service",
        "awslogs-region": "eu-north-1",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }]
}
